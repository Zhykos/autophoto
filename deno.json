{
  "tasks": {
    "scandir": "deno run --allow-read --allow-write --unstable-kv src/main-run-scan.ts",
    "scanschedule": "deno run --allow-read --allow-write --unstable-kv --unstable-cron src/main-schedule-scan.ts",
    "test": "deno test --allow-read --allow-write --trace-leaks --unstable-kv -- config.yml --database=./test/it-database.sqlite3",
    "coverage": "rm -fr coverage && deno test --allow-read --allow-write --unstable-kv --coverage -- config.yml --database=./test/it-database.sqlite3 && deno coverage --html --exclude=\"/test/\" && open coverage/html/index.html",
    "lint": "deno lint && deno run -A npm:@biomejs/biome lint",
    "format": "deno run -A npm:@biomejs/biome check --write"
  },
  "imports": {
    "@p4sca1/cron-schedule": "jsr:@p4sca1/cron-schedule@^5.0.3",
    "@std/assert": "jsr:@std/assert@^1.0.5",
    "@std/cli": "jsr:@std/cli@^1.0.6",
    "@std/crypto": "jsr:@std/crypto@^1.0.3",
    "@std/media-types": "jsr:@std/media-types@^1.0.3",
    "@std/testing": "jsr:@std/testing@^1.0.3",
    "@std/yaml": "jsr:@std/yaml@^1.0.5"
  }
}
